from homeassistant.exceptions import HomeAssistantError as HomeAssistantError
from traceback import FrameSummary
from typing import Callable, Optional, Tuple, TypeVar

CALLABLE_T = TypeVar('CALLABLE_T', bound=Callable)

def get_integration_frame(exclude_integrations: Optional[set]=...) -> Tuple[FrameSummary, str, str]: ...

class MissingIntegrationFrame(HomeAssistantError): ...

def report(what: str) -> None: ...
def report_integration(what: str, integration_frame: Tuple[FrameSummary, str, str]) -> None: ...
def warn_use(func: CALLABLE_T, what: str) -> CALLABLE_T: ...
